[
    {
        "name": "redshift",
        "drivers": {
            "alchemy": "redshift+redshift_connector",
            "jdbc": "jdbc:redshift:iam"
        },
        "spark": {
            "format": "io.github.spark_redshift_community.spark.redshift",
            "options": {
                "forward_spark_s3_credentials": "true"
            }
        }
    },
    {
        "name": "postgres",
        "drivers": {
            "alchemy": "postgresql",
            "jdbc": "jdbc:postgresql"
        },
        "spark": {
            "format": "jdbc",
            "options": {}
        }
    }
]